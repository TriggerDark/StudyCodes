function getData() {
    var ajax;
    if (window.XMLHttpRequest) {
        ajax = new XMLHttpRequest();
    } else if (window.ActiveXObject) {
        ajax = new ActiveXObject("Microsoft.XMLHttp");
    }
    ajax.onreadystatechange = function () {
        var flag = (ajax.readyState==4) && (ajax.status==200);
        if (flag) {
            var  result = ajax.responseText;
            eval("var obj=" +result);
            var sel=document.getElementById("department");
            for(var i=0;i<areas.length;i++){
				sel.innerHTML=sel.innerHTML+"<option value="+areas[i].areaid+">"+areas[i].areaname+"</option>"
			}
        }
    };
    ajax.open("get", "DepartmentEmployee", true);
    ajax.send(null);
}