	<script>
	/*
	遍历 $.each()
	遍历祖先 parent() parents() parentUntil()
	遍历后代 children()直接子元素 find()
	遍历同胞 next() nextAll() nextUntil() prev() prevAll() prevUntil() siblings()所有同胞
	*/
		$(document).ready(function(){
			$('#ratings td').html("<img src='img/starEmpty.jpg'>").mouseOver(function(){
				$(#ratings).html('"<img src='img/starFill.jpg'>"');
			});
		});
	</script>
